<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CollectorSignup">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/boyofaftersignup" />

    <EditText
        android:id="@+id/name111"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:layout_marginTop="192dp"
        android:background="@drawable/plaintextfigure"
        android:backgroundTint="#D9D9D9"
        android:ems="10"
        android:gravity="center"
        android:hint="Type Your Name"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/email111"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:layout_marginTop="376dp"
        android:background="@drawable/plaintextfigure"
        android:backgroundTint="#D9D9D9"
        android:ems="10"
        android:gravity="center"
        android:hint="E-mail Address"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/pass111"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:layout_marginTop="440dp"
        android:background="@drawable/plaintextfigure"
        android:backgroundTint="#D9D9D9"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPassword"
        android:hint="Password"
        android:maxLength="15"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <EditText
        android:id="@+id/retype111"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:layout_marginTop="508dp"
        android:background="@drawable/plaintextfigure"
        android:backgroundTint="#D9D9D9"
        android:ems="10"
        android:gravity="center"
        android:hint="Re-Type Password"
        android:inputType="textPassword"
        android:maxLength="15"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="394dp"
        android:layout_height="141dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:srcCompat="@drawable/efslha" />

    <Button
        android:id="@+id/login111"
        android:layout_width="151dp"
        android:layout_height="41dp"
        android:layout_marginBottom="92dp"
        android:background="@drawable/plaintextfigure"
        android:backgroundTint="#D9D9D9"
        android:fontFamily="@font/inter_semibold"
        android:onClick="login"
        android:text="Login"
        android:textColor="#28A37A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/phone111"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:background="@drawable/plaintextfigure"
        android:ems="10"
        android:gravity="center"
        android:hint="enter number"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/email111"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name111"
        app:layout_constraintVertical_bias="0.829" />

    <TextView
        android:id="@+id/locationEditText"
        android:layout_width="219dp"
        android:layout_height="45dp"
        android:background="@drawable/plaintextfigure"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:hint="please click to get location"
        android:onClick="where"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/phone111"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name111" />
    <ImageView
        android:id="@+id/eyeToggle"
        android:layout_width="36dp"
        android:layout_height="19dp"
        android:layout_gravity="center"
        android:src="@drawable/eyeopen"
        app:layout_constraintBottom_toBottomOf="@+id/pass111"
        app:layout_constraintEnd_toEndOf="@+id/pass111"
        app:layout_constraintHorizontal_bias="0.925"
        app:layout_constraintStart_toStartOf="@+id/pass111"
        app:layout_constraintTop_toTopOf="@+id/pass111"
        app:layout_constraintVertical_bias="0.545" />

    <ImageView
        android:id="@+id/eyeToggle1"
        android:layout_width="36dp"
        android:layout_height="19dp"
        android:layout_gravity="center"
        android:src="@drawable/eyeopen"
        app:layout_constraintBottom_toBottomOf="@+id/retype111"
        app:layout_constraintEnd_toEndOf="@+id/retype111"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/retype111"
        app:layout_constraintTop_toTopOf="@+id/retype111"
        app:layout_constraintVertical_bias="0.538" />

</androidx.constraintlayout.widget.ConstraintLayout>